{
  "name": "docker_web_app",
  "version": "1.0.0",
  "description": "Node.js on Docker",
  "author": "First Last <first.last@example.com>",
  "main": "server.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint",
    "start": "node server.js",
    "start:server": "rm -rf .build/ && yarn run removeDist && yarn run tsc:server && yarn run tsWatch:server",
    "removeDist": "rm -rf server/.dist",
    "tsWatch:server": "tsc-watch -p server --onsuccess \"nodemon --exec babel-node ./server/.dist \"",
    "tsc:server": "tsc -p server",
    "start:client": "webpack-dev-server --config webpack.config.client.js",
    "tsc:init:server": "tsc --init -p server",
    "tsc:init:client": "tsc --init -p client"
  },
  "dependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "apollo-server": "^2.1.0",
    "apollo-server-express": "^2.1.0",
    "babel-loader": "^8.0.2",
    "bcrypt": "^5.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "express": "^4.16.1",
    "graphql": "^14.0.2",
    "graphql-resolvers": "^0.2.2",
    "html-webpack-plugin": "^3.2.0",
    "jsonwebtoken": "^8.3.0",
    "nodemon": "^1.18.4",
    "passport": "^0.4.0",
    "pg": "^7.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sequelize": ">=5.3.0",
    "start-server-webpack-plugin": "^2.2.5",
    "uuid": "^3.3.2",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@types/graphql": "^14.2.0",
    "@types/node": "^11.13.6",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@types/sequelize": "^4.27.48",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "prettier-eslint": "^8.8.2",
    "react-hot-loader": "^4.3.11",
    "ts-loader": "^5.3.3",
    "tsc-watch": "^2.1.2",
    "typescript": "^3.4.4"
  }
}
